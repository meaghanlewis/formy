<h1 align="center">Switch Window</h1>
<form align="center">

  <div class="form-group row">
    <button type="button" class="btn btn-primary" id="new-tab-button" onclick="window.open('https://formy-project.herokuapp.com/');">
      Open new tab
    </button>
  </div>

  <div class="form-group row">
    <button type="button" class="btn btn-primary" id="alert-button" onclick="myFunction()">
    Open alert
    </button>

    <script>
    function myFunction() {
      alert("This is a test alert!");
    }
    </script>
  </div>
</form>
